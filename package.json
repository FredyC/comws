{
  "name": "comws",
  "version": "1.0.0",
  "description": "koa like middlewares for any app",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha -- test/*_test.js test/**/*_test.js --require ./test/init || echo failed",
    "test-shippable": "mkdir -p ./shippable/testresults && istanbul cover --dir ./shippable/codecoverage --report cobertura _mocha -- test/*_test.js test/**/*_test.js --require ./test/init --reporter \"xunit\" > ./shippable/testresults/mocha.xml",
    "build": "babel src/index.js -o dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shes/comws.git"
  },
  "keywords": [
    "co",
    "middlewares",
    "koa",
    "middleware",
    "mw",
    "mws"
  ],
  "devDependencies": {
    "babel": "^4.6.4",
    "chai": "^2.1.0",
    "co": "^4.0.0",
    "co-mocha": "^1.1.0",
    "docflux": "^1.1.2",
    "istanbul": "^0.3.7",
    "mocha": "^2.1.0"
  },
  "dependencies": {
    "is-promise": "^2.0.0",
    "pretty-error": "^1.1.1"
  },
  "author": "andrea@parro.it",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shes/comws/issues"
  },
  "homepage": "https://github.com/shes/comws"
}
